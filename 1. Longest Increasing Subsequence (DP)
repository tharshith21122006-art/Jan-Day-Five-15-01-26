class LIS {
    public static void main(String[] args) {
        int[] a = {10,9,2,5,3,7,101,18};
        int n = a.length;
        int[] dp = new int[n];
        int max = 1;

        for(int i=0;i<n;i++){
            dp[i]=1;
            for(int j=0;j<i;j++)
                if(a[i]>a[j])
                    dp[i]=Math.max(dp[i],dp[j]+1);
            max=Math.max(max,dp[i]);
        }
        System.out.println(max);
    }
}
