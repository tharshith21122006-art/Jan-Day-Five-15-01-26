class Rotate {
    public static void main(String[] args) {
        int[][] m={{1,2},{3,4}};
        int n=m.length;

        for(int i=0;i<n;i++)
            for(int j=i;j<n;j++){
                int t=m[i][j]; m[i][j]=m[j][i]; m[j][i]=t;
            }

        for(int i=0;i<n;i++){
            for(int j=0;j<n/2;j++){
                int t=m[i][j];
                m[i][j]=m[i][n-j-1];
                m[i][n-j-1]=t;
            }
        }

        for(int[] r:m){
            for(int x:r) System.out.print(x+" ");
            System.out.println();
        }
    }
}
