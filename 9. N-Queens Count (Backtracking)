class NQueen {
    static int count=0;

    static void solve(int r, boolean[] col, boolean[] d1, boolean[] d2, int n){
        if(r==n){count++; return;}
        for(int c=0;c<n;c++){
            if(!col[c] && !d1[r-c+n] && !d2[r+c]){
                col[c]=d1[r-c+n]=d2[r+c]=true;
                solve(r+1,col,d1,d2,n);
                col[c]=d1[r-c+n]=d2[r+c]=false;
            }
        }
    }

    public static void main(String[] args) {
        int n=4;
        solve(0,new boolean[n],new boolean[2*n],new boolean[2*n],n);
        System.out.println(count);
    }
}
