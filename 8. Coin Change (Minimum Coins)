class CoinChange {
    public static void main(String[] args) {
        int[] coins={1,2,5};
        int amt=11;
        int[] dp=new int[amt+1];
        Arrays.fill(dp,1000);
        dp[0]=0;

        for(int c:coins)
            for(int i=c;i<=amt;i++)
                dp[i]=Math.min(dp[i],dp[i-c]+1);

        System.out.println(dp[amt]);
    }
}
